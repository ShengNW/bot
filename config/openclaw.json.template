{
  "meta": {
    "lastTouchedVersion": "2026.2.26"
  },
  "models": {
    "mode": "merge",
    "providers": {
      "router": {
        "baseUrl": "${ROUTER_BASE_URL}",
        "apiKey": "${ROUTER_API_KEY}",
        "auth": "api-key",
        "api": "openai-responses",
        "models": [
          {
            "id": "${ROUTER_MODEL}",
            "name": "${ROUTER_MODEL}"
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "router/${ROUTER_MODEL}"
      },
      "compaction": {
        "mode": "safeguard"
      }
    }
  },
  "messages": {
    "groupChat": {
      "mentionPatterns": [
        ".*"
      ],
      "historyLimit": 30
    }
  },
  "channels": {
    "whatsapp": {
      "enabled": true,
      "dmPolicy": "pairing",
      "groupPolicy": "open",
      "debounceMs": 0,
      "accounts": {
        "default": {
          "enabled": true,
          "dmPolicy": "pairing",
          "groupPolicy": "open",
          "debounceMs": 0
        }
      },
      "mediaMaxMb": 50
    }
  },
  "gateway": {
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  },
  "plugins": {
    "entries": {
      "whatsapp": {
        "enabled": true
      }
    }
  }
}
